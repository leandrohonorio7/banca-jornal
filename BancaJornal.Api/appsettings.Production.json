{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning",
      "Microsoft.AspNetCore.HealthChecks": "Information"
    },
    "File": {
      "Path": "/app/logs/bancajornal-.log",
      "RollingInterval": "Day",
      "FileSizeLimitBytes": 10485760,
      "RetainedFileCountLimit": 7
    }
  },
  "AllowedHosts": "*",
  "PathBase": "/bancajornal",
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=/app/data/bancajornal.db"
  },
  "Cors": {
    "AllowedOrigins": [
      "*"
    ]
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:80"
      }
    }
  },
  "ForwardedHeaders": {
    "ForwardedForHeaderName": "X-Forwarded-For",
    "ForwardedProtoHeaderName": "X-Forwarded-Proto",
    "ForwardLimit": 1,
    "KnownNetworks": [],
    "KnownProxies": []
  }
}
